                                                <img src="<%- MEDIA_URL %>/assets/images/icon/icon-2-5.webp"
                                                     width="70" height="70" alt="icon">
                                            </div>
                                            <div class="box-content">
                                                <p>GST</p>
                                                <p>certificate</p>
                                            </div>
                                        </div>
                                    </div>
                                </a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>

